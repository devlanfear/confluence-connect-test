{
    "key": "atlassian-connect-gmaps",
    "name": "Google Maps",
    "description": "Google Maps plugin for connect",
    "version": "1.0.4",
    "vendor": {
        "name": "Atlassian",
        "url": "https://developer.atlassian.com"
    },
    "baseUrl": "http://acgmaps.bitbucket.org",
    "authentication": {
        "type": "none"
    },
    "modules": {
        "dynamicContentMacros": [
            {
                "name": {
                    "i18n": "gmaps.macro.name",
                    "value": "Google Map"
                },
                "featured": true,
                "url": "/display.html",
                "icon": {
                    "height": 80,
                    "url": "/favicon.png",
                    "width": 80
                },
                "width": "500px",
                "height": "200px",
                "description": {
                    "i18n": "gmaps.macro.desc",
                    "value": "Insert a Google map into a page"
                },
                "editor": {
                    "url": "/editor.html",
                    "width": "865px",
                    "height": "600px"
                }
            }
        ]
    }
}
